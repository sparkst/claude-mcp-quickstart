{
  "name": "claude-mcp-quickstart",
  "version": "2.2.10",
  "description": "MCP Quickstart with expert multi-disciplinary AI assistance",
  "main": "index.js",
  "type": "module",
  "bin": {
    "claude-mcp-quickstart": "index.js"
  },
  "files": [
    "index.js",
    "setup.js",
    "dev-mode.js",
    "templates/",
    "bootstrap-lovable-v2/",
    "test-utils.js"
  ],
  "scripts": {
    "setup": "node setup.js",
    "dev-mode": "node dev-mode.js",
    "verify": "node index.js verify",
    "quick-start": "node index.js quick-start",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint *.js",
    "format": "prettier --write *.js",
