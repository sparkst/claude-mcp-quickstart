    "inquirer": "^9.2.12",
    "ora": "^6.3.1",
    "tavily-mcp": "latest"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "eslint": "^9.35.0",
    "prettier": "^3.6.2",
    "tmp-promise": "^3.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "mcp",
    "claude",
    "ai",
    "automation",
    "development",
    "quickstart"
